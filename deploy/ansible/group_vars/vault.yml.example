# Copie este arquivo para vault.yml e criptografe com:
# ansible-vault encrypt vault.yml
#
# Para editar depois:
# ansible-vault edit vault.yml

# IMPORTANTE: Use senhas fortes e únicas!
# NÃO use os valores de exemplo abaixo em produção!

# Senha do PostgreSQL
vault_db_password: "CHANGE_ME_STRONG_PASSWORD"

# NextAuth Secret (gere com: openssl rand -base64 32)
vault_nextauth_secret: "GENERATE_WITH_openssl_rand_-base64_32"

# Senhas dos usuários
vault_admin_password: "CHANGE_ME_ADMIN_PASSWORD"
vault_sdr_password: "CHANGE_ME_SDR_PASSWORD"
